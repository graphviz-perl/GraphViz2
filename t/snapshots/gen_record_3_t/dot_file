 "Alphabet":"port_a" -> "Alphabet":"port_n" [ color="maroon" ]
 "Alphabet":"port_b" -> "Alphabet":"port_s" [ color="blue" ]
 "Alphabet":"port_d" -> "Alphabet":"port_w" [ color="red" ]
