'digraph Perl {
 graph [ label="Record demo 4 - Set record shapes in various ways" ]
 node [ color="magenta" ]
 "One" [ label="" shape="Mrecord" ]
 "Two" [ label="<port1> Left|<port2> Right" shape="Mrecord" ]
 "Three" [ color="black" label="<port1> Good|<port2> Bad" shape="record" ]
 "Four" [ label="{Big|Small}" shape="Mrecord" ]
 "Five" [ label="{Yin|Yang}" shape="record" ]
}
'
