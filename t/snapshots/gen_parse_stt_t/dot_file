'digraph Perl {
 graph [ rankdir="TB" ]
 node [ color="blue" shape="oval" ]
 edge [ color="grey" ]
 graph [ clusterrank="local" compound="1" rankdir="TB" ]

 subgraph "cluster_attribute" {
  rank="TB"
  "attribute"
  "[\\w :;]+:attribute"
 }

 subgraph "cluster_class" {
  rank="TB"
  "class"
  "[a-z][a-z0-9_]*:class"
  "\\s+:class"
 }

 subgraph "cluster_class_attribute" {
  rank="TB"
  "class_attribute"
  "[\\w :;]+:class_attribute"
 }

 subgraph "cluster_class_daisy_chain" {
  rank="TB"
  "class_daisy_chain"
  ",:class_daisy_chain"
  "\\s+:class_daisy_chain"
 }

 subgraph "cluster_daisy_chain" {
  rank="TB"
  "daisy_chain"
  ",:daisy_chain"
  "\\s+:daisy_chain"
 }

 subgraph "cluster_edge" {
  rank="TB"
  "edge"
  "[-<=>~.]+:edge"
  "\\s+:edge"
 }

 subgraph "cluster_global" {
  rank="TB"
  "global"
  "\\s+:global"
 }

 subgraph "cluster_group" {
  rank="TB"
  "group"
  "[\\w.]+::group"
  "\\s+:group"
 }

 subgraph "cluster_node" {
  rank="TB"
  "node"
  "[\\w.]+:node"
  "\\[:node"
  "\\s+:node"
 }

 subgraph "cluster_post_attribute" {
  rank="TB"
  "post_attribute"
  "\\s+:post_attribute"
  "}:post_attribute"
 }

 subgraph "cluster_post_class_attribute" {
  rank="TB"
  "post_class_attribute"
  "\\s+:post_class_attribute"
  "}:post_class_attribute"
 }

 subgraph "cluster_post_group" {
  rank="TB"
  "post_group"
  "\\):post_group"
  "\\s+:post_group"
 }

 subgraph "cluster_post_node" {
  rank="TB"
  "post_node"
  "\\s+:post_node"
  "]:post_node"
 }

 subgraph "cluster_start_attribute" {
  rank="TB"
  "start_attribute"
  "{:start_attribute"
 }

 subgraph "cluster_start_class_attribute" {
  rank="TB"
  "start_class_attribute"
