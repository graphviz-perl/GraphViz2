 }

 subgraph "cluster_start_node" {
  rank="TB"
  "start_node"
  "\\[:start_node"
  "\\s+:start_node"
 }

 subgraph "cluster_start_subclass" {
  rank="TB"
  "start_subclass"
  "\\.:start_subclass"
 }

 subgraph "cluster_subclass" {
  rank="TB"
  "subclass"
  "[a-z][a-z0-9_]*:subclass"
  "\\.:subclass"
  "\\s+:subclass"
 }
 ",:class_daisy_chain" [ color="black" label="/,/" shape="box" ]
 ",:class_daisy_chain" -> "class_daisy_chain"
 ",:daisy_chain" [ color="black" label="/,/" shape="box" ]
 ",:daisy_chain" -> "daisy_chain"
