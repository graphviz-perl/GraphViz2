'digraph Perl {
 graph [ rankdir="TB" ]
 node [ color="blue" shape="oval" ]
 edge [ color="grey" ]
 "1" [ label="START \\$1" ]
 "1" -> "3"
 "3" [ label="" shape="diamond" ]
 "3" -> "4"
 "3" -> "20"
 "20" [ label="START \\$3" ]
 "20" -> "22"
 "22" [ label="foo" shape="box" ]
 "22" -> "24"
 "24" [ label="END \\$3" ]
 "24" -> "26"
 "26" [ label="END \\$1" ]
 "26" -> "28"
 "28" [ label="END" ]
 "4" [ label="START \\$2" ]
 "4" -> "6"
 "6" [ label="\\[0-9a-d\\]" shape="box" ]
 "6" -> "17"
 "17" [ label="END \\$2" ]
 "17" -> "26"
}
'
