'digraph Perl {
 graph [ rankdir="TB" ]
 node [ shape="oval" ]
 edge [ color="grey" ]
 "Embedded\\nnewline\\nnode\\nname"
 "Embedded newline label name" [ label="Embedded\\nnewline\\nlabel" ]
 "Embedded\\ndouble-quote\\nnode\\nname\\n\\""
 "Embedded\\double-quote\\label" [ label="Embedded\\ndouble-quote\\nlabel\\n\\"" ]
 "Line justification 1" [ label="A short line\\rA much longer line" ]
 "Line justification 2" [ label="A much longer line\\rA short line" ]
}
'
